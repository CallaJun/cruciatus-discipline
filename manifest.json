{
  "manifest_version": 2,
  "name": "Cruciatus Discipline",
  "version": "0.1",
  "browser_action":
  {
  "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-2.1.4.min.js", "content.js"] 
    }
  ]
}